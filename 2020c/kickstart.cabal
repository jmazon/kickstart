  ghc-options: -Wall

executable C-perfect-subarray
  main-is:             C-perfect-subarray.hs
  build-depends:       base,bytestring,vector
  default-language:    Haskell2010
  ghc-options: -Wall

executable D-candies
  main-is:             D-candies.hs
  build-depends:       base,vector
