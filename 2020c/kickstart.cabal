name:                kickstart
version:             2020.3.0.0
synopsis:            Kickstart 2020 C
license:             AllRightsReserved
author:              Jean-Baptiste Mazon
maintainer:          author
category:            Competition
build-type:          Simple
cabal-version:       >=1.10

executable A-countdown
  main-is:             A-countdown.hs
  build-depends:       base
  default-language:    Haskell2010
  ghc-options: -Wall

executable B-stable-wall
